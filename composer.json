{
	"name": "undkonsorten/typo3-cute-mailing",
	"type": "typo3-cms-extension",
	"description": "A TYPO3 extension that makes mailing and newsletter sending cute. Send TYPO3 pages as emails to many recipients.",
	"homepage": "https://www.undkonsorten.com",
	"authors": [
		{
			"name": "Eike Starkmann",
			"email": "starkmann@undkonsorten.com",
			"homepage": "http://undkonsorten.com",
			"role": "Developer"
		}
	],
	"license": [
		"GPL-2.0+"
	],
	"keywords": [
		"email",
		"newsletter",
		"TYPO3"
	],
	"support": {
		"issues": ""
	},
	"repositories": [
		{
			"type": "vcs",
			"url": ""
		}
	],
	"require": {
		"ext-json": "*",
		"php": "^7.4|^8.1",
		"typo3/cms-backend": "^11.5",
		"typo3/cms-core": "^11.5",
		"typo3/cms-extbase": "^11.5",
		"undkonsorten/taskqueue": "^8.0"
	},
	"suggest": {
		"undkonsorten/typo3-cute-mailing-registeradddess": "For register/unregister subscriber with registeraddess",
		"undkonsorten/typo3-cute-mailing-ttaddress": "For tt_address as recipient connector"
	},
	"autoload": {
		"psr-4": {
			"Undkonsorten\\CuteMailing\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Undkonsorten\\CuteMailing\\Tests\\": "Tests/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "cute_mailing"
		}
	}
}
